{
  "name": "@stayradiated/shoebox",
  "version": "2.6.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "tidy": "xo --fix",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@iarna/toml": "2.2.5",
    "@thi.ng/dgraph": "2.1.56",
    "chalk": "5.3.0",
    "escape-string-regexp": "5.0.0",
    "execa": "7.2.0",
    "flexver": "1.1.1",
    "handlebars": "4.7.8",
    "mem": "9.0.2",
    "multimatch": "6.0.0",
    "p-map": "6.0.0",
    "p-queue": "7.3.4",
    "tempy": "3.1.0",
    "undici": "5.23.0",
    "walkdir": "0.4.1",
    "yargs": "17.7.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/node": "20.4.10",
    "@types/yargs": "17.0.24",
    "typescript": "5.1.6",
    "xo": "0.56.0"
  },
  "bin": {
    "shoebox": "./bin/shoebox"
  },
  "__comment": "The following configs are managed by `@stayradiated/jsconfig`",
  "xo": {
    "space": true,
    "semicolon": false,
    "prettier": true,
    "rules": {
      "n/file-extension-in-import": "off"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
